      "url": "https://www.paypal.me/xan105"
    },
    {
      "type": "patreon",
      "url": "https://www.patreon.com/xan105"
    }
  ],
  "devDependencies": {
    "@electron/asar": "^3.2.18",
    "@electron/fuses": "^1.8.0",
    "@eslint/js": "^8.56.0",
    "cli-progress": "^3.12.0",
    "electron": "^34.0.2",
    "electron-context-menu": "^4.0.4",
    "eslint": "^8.56.0",
    "globals": "^14.0.0"
  },
  "dependencies": {
    "@xan105/error": "^1.7.1",
    "@xan105/ffi": "^1.1.3",
    "@xan105/fs": "^2.2.1",
    "@xan105/ini": "^2.2.0",
    "@xan105/is": "^2.9.3",
    "@xan105/usershellfolder": "^1.2.1",
    "@xan105/vanilla-query": "^1.8.0",
